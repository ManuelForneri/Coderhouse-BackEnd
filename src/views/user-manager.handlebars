<div class="card">
  <table id="dinamic-list-products" class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Id</th>
        <th scope="col">Titulo</th>
        <th scope="col">Precio</th>
        <th scope="col">Descripcion</th>
        <th scope="col">URL Imagen</th>
        <th scope="col">Codigo</th>
        <th scope="col">Stock</th>
        <th scope="col">Categoria</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
        <tr>
          <th scope="row">*</th>
          <td>{{this._id}}</td>
          <td>{{this.title}}</td>
          <td>{{this.price}}</td>
          <td>{{this.description}}</td>
          <td>{{this.thumbnail}}</td>
          <td>{{this.code}}</td>
          <td>{{this.stock}}</td>
          <td>{{this.category}}</td>
        </tr>

      {{/each}}
    </tbody>
  </table>
  <a class="btn btn-primary" href="/admin/clear-list">Limpiar lista de usuarios
    (2 dias)</a>

</div>